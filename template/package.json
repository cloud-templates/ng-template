{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "private": true,
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean:dev": "rimraf dev",
    "clean:dll": "rimraf src/assets/dll",
    "build": "set PRODUCTION=1 && npm run build:production",
    "test": "set TEST=1 && npm run build:production",
    "dev": "node npm-scripts/clean-dev.js && set DEV=1 && webpack --config build/webpack/development.js --progress --watch",
    "build:production": "node npm-scripts/clean-build.js && webpack --config build/webpack/production.js --progress --bail",
    "dll": "node npm-scripts/clean-dll.js && webpack --config build/webpack/dll.js --progress --colors"
  },
